<Schema name="dataperu" default_locale="es">
    <Cube name="itp_cite_descriptivo">
        <Annotation name="source_name">ITP</Annotation>
        <Annotation name="source_description">Instituto Tecnol贸gico de la Producci贸n</Annotation>
        <Annotation name="dataset_name">Instituto Tecnol贸gico de la Producci贸n</Annotation>
        <Annotation name="dataset_link">http://www.gob.pe/itp</Annotation>

        <Table name="itp_cite_descriptivo" primary_key="cite_id"/>

        <DimensionUsage foreign_key="cite_id" source="CITE" name="CITE" />
        <DimensionUsage foreign_key="district_id" source="Ubigeo" name="Ubigeo" />
        <DimensionUsage foreign_key="cadena_atencion_id" source="Cadena CITE" name="Cadena CITE" />

        <Dimension name="Tipo CITE" foreign_key="tipo_id">
            <Hierarchy name="Tipo CITE" primary_key="id">
                <InlineTable alias="dim_inline_tipo_id_cite">
                    <ColumnDef name="id" key_type="nontext" />
                    <ColumnDef name="name" key_type="text" />
                    <Row>
                        <Value column="id">1</Value>
                        <Value column="name">CITE</Value>
                    </Row>
                    <Row>
                        <Value column="id">2</Value>
                        <Value column="name">UT</Value>
                    </Row>

                </InlineTable>
                <Level name="Tipo CITE" key_column="id" name_column="name" />
            </Hierarchy>
        </Dimension>


        <Dimension name="CITE Slug" foreign_key="cite_slug">
            <Hierarchy name="CITE Slug">
                <Level name ="CITE Slug" key_column="cite_slug" key_type="text" />
            </Hierarchy>
        </Dimension>


        <Dimension name="Latitud" foreign_key="latitud">
            <Hierarchy name="Latitud">
                <Level name ="Latitud" key_column="latitud" key_type="text" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Longitud" foreign_key="longitud">
            <Hierarchy name="Longitud">
                <Level name ="Longitud" key_column="longitud" key_type="text" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Descriptivo" foreign_key="longitud">
            <Hierarchy name="Descriptivo">
                <Level name ="Descriptivo" key_column="descriptivo" key_type="text" />
             </Hierarchy>
        </Dimension>

        <Measure name="Cantidad CITE" column="cantidad_cite" aggregator="count" />

    </Cube>
</Schema>       