        <Measure name="Porcentaje morosidad de crédito de la banca" column="perc_morosidad_credi_banca_mult" aggregator="sum" />

        <Measure name="Visitas Museo Nacional de arqueología e historia del Peru" column="m_nacional_arquelogia_historia_Peru" aggregator="sum" />
