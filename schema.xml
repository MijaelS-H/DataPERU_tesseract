<Schema name="dataperu">

	<SharedDimension name="CIIU" type="standard">
        <Hierarchy name="CIIU">
            <Table name="dim_shared_ciiu" />

            <Level name="Seccion" key_column="section_id" name_column="section_name" key_type="text" />
            <Level name="Division" key_column="division_id" name_column="division_name" key_type="text" />
            <Level name="Grupo" key_column="group_id" name_column="group_name" key_type="text" />
            <Level name="Clase" key_column="class_id" name_column="class_name" key_type="text" />
        </Hierarchy>
    </SharedDimension>
    <SharedDimension name="Ubigeo" type="geo">
        <Hierarchy name="Ubigeo">
            <Table name="dim_shared_ubigeo" />

            <Level name="Nacion" key_column="nation_id" name_column="nation_name" key_type="text" />
            <Level name="Departamento" key_column="department_id" name_column="department_name" key_type="text" />
            <Level name="Provincia" key_column="province_id" name_column="province_name" key_type="text" />
            <Level name="Distrito" key_column="district_id" name_column="district_name" key_type="text" />
        </Hierarchy>
    </SharedDimension>

	<Cube name="itp_indicators_y_act_dept">

      <Annotation name="topic">Economia</Annotation>
      <Annotation name="subtopic">Economia</Annotation>
      <Annotation name="source_name">Indicadores ITP</Annotation>

    <Table name="itp_indicators_y_act_dept" primary_key="ubigeo" />

    <DimensionUsage foreign_key="ubigeo" source="Ubigeo" name="geo" />

    <Dimension name="Year" foreign_key="year">
      <Hierarchy name="Year">
	  	  <Level name="Year" key_column="year" />
      </Hierarchy>
    </Dimension>

    <Dimension name="Actividad economica" foreign_key="act_economica">
      <Hierarchy name="Actividad economica" primary_key="id">
        <InlineTable alias="dim_inline_act_economica">
          <ColumnDef name="id" key_type="nontext" />
          <ColumnDef name="name" key_type="text" />
          <Row>
            <Value column="id">1</Value>
            <Value column="name">Agricultura, ganaderia, caza y silvicultura</Value>
          </Row>
          <Row>
            <Value column="id">2</Value>
            <Value column="name">Pesca y acuicultura</Value>
          </Row>
          <Row>
            <Value column="id">3</Value>
            <Value column="name">Extraccion de petroleo, gas, minerales y servicios conexos</Value>
          </Row>
          <Row>
            <Value column="id">4</Value>
            <Value column="name">Electricidad, gas y agua</Value>
          </Row>
          <Row>
            <Value column="id">5</Value>
            <Value column="name">Construccion</Value>
          </Row>
          <Row>
            <Value column="id">6</Value>
            <Value column="name">Comercio, mantenimiento y reparacion de vehiculos automotores y motocicletas</Value>
          </Row>
          <Row>
            <Value column="id">7</Value>
            <Value column="name">Transporte, almacenamiento, correo y mensajeria</Value>
          </Row>
          <Row>
            <Value column="id">8</Value>
            <Value column="name">Alojamiento y restaurantes</Value>
          </Row>
          <Row>
            <Value column="id">9</Value>
            <Value column="name">Telecomunicaciones y otros servicos de informacion</Value>
          </Row>
          <Row>
            <Value column="id">10</Value>
            <Value column="name">Administración publica y defensa</Value>
          </Row>
          <Row>
            <Value column="id">11</Value>
            <Value column="name">Otros servicios</Value>
          </Row>
        </InlineTable>
        <Level name="Actividad economica" key_column="id" name_column="name" />
      </Hierarchy>
    </Dimension>

    <Measure name="Linea de pobreza alimentaria" column="valor_agregado_bruto_2007" aggregator="sum" />
    <Measure name="Linea de pobreza total" column="valor_agregado_bruto_cte" aggregator="sum" />

  </Cube>
<Cube name="inei_renamu_dept">
        <Annotation name="source_name">INEI</Annotation>
        <Annotation name="source_description">Instituto Nacional de Estadística e Informática</Annotation>
        <Annotation name="dataset_name">Registro Nacional de Municipalidades 2019</Annotation>
        <Annotation name="dataset_link">https://webinei.inei.gob.pe/anda_inei/index.php/catalog/709</Annotation>

        <Table name="inei_renamu_dept" primary_key="ubigeo" />

        <DimensionUsage foreign_key="ubigeo" source="Ubigeo" name="Ubigeo" />

        <Dimension name="Year" foreign_key="anio">
            <Hierarchy name="Year">
                <Level name="Year" key_column="anio" />
            </Hierarchy>
        </Dimension>

        <Measure name="Numero Autos y Camionetas Operativos " column="n_autos_operativos" aggregator="sum" /> 
    </Cube>

</Schema>